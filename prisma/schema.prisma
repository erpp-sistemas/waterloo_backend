generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
  // provider = "postgresql"
  // url      = env("POSTGRES_URL")
}

model todo {
  id          Int      @id @default(autoincrement())
  text        String   @db.NVarChar(100)
  completedAt DateTime? @db.DateTime()
}
